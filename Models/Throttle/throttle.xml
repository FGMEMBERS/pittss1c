<?xml version="1.0"?>

<PropertyList>

  <path>throttle.ac</path>

  <animation>
    <type>rotate</type>
    <object-name>Throttle</object-name>
    <property>controls/engines/engine[0]/throttle</property>
    <factor>-60</factor>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
</PropertyList>
